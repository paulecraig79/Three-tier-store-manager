<Window x:Class="SA_Three_Tiered.Inventory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SA_Three_Tiered"
        mc:Ignorable="d"
        Title="Inventory" Height="250" Width="600">
    <Grid>
        <Label Content="Inventory" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="22"/>
        <Button x:Name="Search" Content="Search" HorizontalAlignment="Left" Margin="47,0,0,0" VerticalAlignment="Center" Height="63" Width="135" Click="Search_Click"/>
        <Button x:Name="Order_button" Content="Order" HorizontalAlignment="Center" VerticalAlignment="Center" Width="136" Height="63" Click="Order_button_Click"/>
        <Button x:Name="Pricing_Button" Content="Pricing" HorizontalAlignment="Right" Margin="0,0,49,0" VerticalAlignment="Center" Width="135" Height="63" Click="Pricing_button_Click"/>
    </Grid>
</Window>
